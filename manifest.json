{
    "name": "Chrome Skeleton Project",
    "description": "Example of a chrome skeleton project",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
      ],
    "browser_action": {
        "default_icon": "images/email_icon.png",
        "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }